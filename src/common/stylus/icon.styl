        url('fonts/muum.ttf?yxlr5f') format('truetype'),
        url('fonts/muum.woff?yxlr5f') format('woff'),
        url('fonts/muum.svg?yxlr5f#muum') format('svg')
  font-weight: normal
  font-style: normal

[class^="icon-"], [class*=" icon-"]
  /* use !important to prevent issues with browser extensions that change fonts */
